{
    "name": "jayzeng/virustotalapi",
    "description": "VirusTotal Public API v2.0 PHP Wrapper",
    "type": "library",
    "keywords": ["virus","virustotal", "api", "php", "wrapper", "security"],
    "homepage": "https://github.com/jayzeng/virustotal_apiwrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Jay Zeng",
            "email": "jayzeng@jay-zeng.com",
            "homepage": "http://jay-zeng.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "guzzle/guzzle": ">=3.6"
    },
    "require-dev": {
       "phpunit/phpunit": ">=4.0"
    },
    "autoload": {
        "psr-0": {
            "VirusTotal": "Src/",
            "Tests\\VirusTotal": "Tests/"
        }
    },
    "config": {
        "vendor-dir" : "Vendors",
        "vendor-bin" : "Vendors/bin"
    }
}
